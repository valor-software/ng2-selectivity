{
  "name": "ng2-selectivity",
  "version": "0.0.0",
  "description": "angular2 selectivity directive",
  "scripts": {
    "deploy": "NODE_ENV=production webpack -p --progress --color --optimize-minimize --optimize-dedupe --optimize-occurence-order",
    "prepublish": "gulp clean && ntsc && mkdir -p dist/css && cp components/css/ng2-selectivity.css dist/css",
    "prestart": "npm install",
    "server": "webpack-dev-server --hot --inline --colors --display-error-details --display-cached",
    "start": "npm run server",
    "test": "gulp lint"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "components"
  ],
  "keywords": [
    "angular2",
    "angularjs"
  ],
  "author": "Vyacheslav Chub <vyacheslav.chub@valor-software.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/valor-software/ng2-selectivity.git"
  },
  "bugs": {
    "url": "https://github.com/valor-software/ng2-selectivity/issues"
  },
  "homepage": "https://github.com/valor-software/ng2-selectivity#readme",
  "peerDependencies": {
    "angular2": "2.0.0-beta.2",
    "es6-shim": "^0.33.3",
    "rxjs": "5.0.0-beta.0",
    "zone.js": "0.5.10"
  },
  "devDependencies": {
    "bootstrap": "3.3.5",
    "clean-webpack-plugin": "0.1.3",
    "compression-webpack-plugin": "0.2.0",
    "eslint": "1.6.0",
    "exports-loader": "0.6.2",
    "file-loader": "0.8.4",
    "gulp": "3.9.0",
    "gulp-clean": "0.3.1",
    "gulp-eslint": "1.0.0",
    "gulp-size": "2.0.0",
    "gulp-tsc": "1.1.1",
    "gulp-tslint": "3.3.1",
    "html-loader": "0.3.0",
    "markdown-loader": "0.1.7",
    "marked": "0.3.5",
    "moment": "2.10.6",
    "ntypescript": "1.201602032217.1",
    "pre-commit": "1.1.1",
    "prismjs": "valorkin/prism",
    "prismjs-loader": "0.0.2",
    "raw-loader": "0.5.1",
    "reflect-metadata": "0.1.2",
    "require-dir": "0.3.0",
    "ts-loader": "0.5.6",
    "typescript": "1.7.5",
    "webpack": "1.12.2",
    "webpack-dev-server": "1.12.0"
  }
}
